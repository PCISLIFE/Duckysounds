{
  "manifest_version": 3,
  "name": "Duck Click Quacker",
  "version": "1.0.0",
  "description": "Random duck sounds when you click anywhere on a webpage (random intervals).",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["duck1.mp3", "duck2.mp3", "duck3.mp3"],
      "matches": ["<all_urls>"]
    }
  ]
}
